{
  "name": "static-page-template",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && cp dist/index.html dist/404.html",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src/",
    "fmt": "biome format --write src/",
    "fmt:check": "biome format src/",
    "e2e-test": "playwright test",
    "e2e-test:ui": "playwright test --ui",
    "e2e-test:headed": "playwright test --headed",
    "ct-test": "playwright test -c playwright-ct.config.ts",
    "ct-test:ui": "playwright test -c playwright-ct.config.ts --ui",
    "ct-test:headed": "playwright test -c playwright-ct.config.ts --headed",
    "test": "vitest run",
    "test:coverage": "vitest --coverage"
  },
  "engines": {
    "node": ">=24",
    "pnpm": ">=10"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "1.31.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "uplot": "1.6.32",
    "wouter": "3.7.1",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@playwright/experimental-ct-react": "1.56.1",
    "@playwright/test": "1.56.1",
    "@tailwindcss/vite": "4.1.17",
    "@types/emscripten": "1.41.5",
    "@types/node": "24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.1.0",
    "fast-check": "4.3.0",
    "playwright": "1.56.1",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.3",
    "vite": "7.2.2",
    "vitest": "4.0.8"
  }
}